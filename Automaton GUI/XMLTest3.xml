<?xml version="1.0" encoding="utf-8"?>
<Root xmlns="http://tempuri.org/XMLSchemaRegExp.xsd">
  <!-- regular expression: (0|1)0[0-9] -->
  <!-- accepted strings:  000 001 102 003 009 -->
  <!-- rejected strings: 00 010 0012 0077 209 -->
  <Or>
    <String>0</String>
    <String>1</String>
  </Or>
  <String>0</String>
  <AnySymbol>0123456789</AnySymbol>    
</Root>